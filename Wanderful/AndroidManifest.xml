<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appliance.wanderful"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.appliance.wanderful.SearchEvent"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.appliance.wanderful.MainSchedule"
            android:label="@string/title_activity_main_schedule"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.appliance.wanderful.MySchedule"
            android:label="@string/title_activity_my_schedule"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.appliance.wanderful.Map"
            android:label="@string/title_activity_map"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.appliance.wanderful.HashFeed"
            android:label="@string/title_activity_hash_feed"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.appliance.wanderful.DetailActivity"
            android:label="@string/title_activity_detail"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.appliance.wanderful.SearchPage"
            android:label="@string/title_event_list"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.appliance.wanderful.EventListActivity"
            android:label="@string/title_event_list"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.appliance.wanderful.EventDetailActivity"
            android:label="@string/title_event_detail"
            android:parentActivityName=".EventListActivity"
            android:screenOrientation="portrait" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:screenOrientation="portrait"
                android:value=".EventListActivity" />
        </activity>
        <activity
            android:name="com.appliance.wanderful.YoutubeActivity"
            android:label="@string/title_activity_youtube" >
        </activity>
        <activity
            android:name="com.appliance.wanderful.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>
